{
  "name": "hexaa/hexaa-backend",
  "license": "Apache-2.0",
  "type": "project",
  "description": "HEXAA virtual organization management software",
  "autoload": {
    "psr-0": {"": "src/"}
  },
  "require": {
    "php": ">=5.3.9",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-xml": "*",
    "ext-pdo_mysql": "*",
    "ext-memcached": "*",
    "ext-openssl": "*",
    "lib-curl": "*",
    "symfony/symfony": "~2.3",
    "symfony/icu": "~1.2.2",
    "doctrine/orm": "*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/dbal": "*",
    "twig/extensions": "~1.3.0",
    "symfony/assetic-bundle": "~2.7",
    "symfony/swiftmailer-bundle": "~2.3.8",
    "symfony/monolog-bundle": "~2.8.2",
    "sensio/distribution-bundle": "~5.0.1",
    "sensio/framework-extra-bundle": "~3.0.11",
    "sensio/generator-bundle": "~3.0.0",
    "jms/serializer-bundle": "~1.1.0",
    "friendsofsymfony/rest-bundle": "~1.7.2",
    "nelmio/api-doc-bundle": "~2.10.3",
    "friendsofsymfony/http-cache-bundle": "*",
    "willdurand/hateoas-bundle": "dev-master#3d46f0a8ee6287a01edcd2d5eeae0e8b95aab2b7",
    "hautelook/templated-uri-bundle": "~2.0.0",
    "willdurand/rest-extra-bundle": "~1.0.2",
    "egulias/email-validator": "~1.2.11",
    "ramsey/uuid": "~3.0.0",
    "nelmio/cors-bundle": "~1.4.0",
    "jms/translation-bundle": "~1.1.0",
    "moontoast/math": "~1.1.0"
  },
  "conflict": {
    "symfony/routing": "2.4.1",
    "twig/twig": "~2.0"
  },
  "scripts": {
    "pre-install-cmd": [
      "bash create_config.sh"
    ],
    "pre-update-cmd": [
      "bash create_config.sh"
    ],
    "post-install-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
    "post-update-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "stable",
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web"
  }
}
